<h2>Ваша корзина</h2>

	<table class="table table-striped">
		<thead>
			<tr>
				<th>Название</th>
				<th>Цена</th>
				<th>шт.</th>
        	</tr>
        </thead>
        <tbody>
        	<% @items.each do |item| %>
        	<tr>
		        <td><%= item[0].title %> </td>
		        <td><%= item[0].price %></td>
		        <td><%= item[1] %></td>

        	</tr>
        	 <% end %>
	    </tbody>
	</table>
